{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/github", {
      "assets": [
        {"path": "dist/dns-benchmark-linux-amd64"},
        {"path": "dist/dns-benchmark-linux-arm64"},
        {"path": "dist/dns-benchmark-darwin-amd64"},
        {"path": "dist/dns-benchmark-darwin-arm64"},
        {"path": "dist/dns-benchmark-windows-amd64.exe"},
        {"path": "dist/dns-benchmark-windows-arm64.exe"},
        {"path": "dist/checksums.txt"}
      ]
    }],
    ["@semantic-release/git", {
      "assets": ["go.mod", "go.sum"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }]
  ]
}
